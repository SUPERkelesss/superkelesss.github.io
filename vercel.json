{
    "builds": [
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": {
                "disDir": "Site"
            }
        }
    ],
    "build": {
        "env": {
            "PYTHON_VERSION": "3.9"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/sute/$1"
        }
    ]

}